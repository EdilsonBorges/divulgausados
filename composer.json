{
  "name": "divulgausados/divulgausados",
  "version": "0.0.2",
  "homepage": "https://github.com/murilocosta/divulgausados",
  "author": "Murilo Nunes José da Costa <murilo.nunes.jose@gmail.com>",
  "description": "Uma aplicação para compartilhar e divulgar dados de veículos usados.",
  "keywords": [
    "divulgacao", 
    "veiculos", 
    "automoveis", 
    "carro", 
    "usados"
  ],
  "license": "MIT",
  "require": {
    "laravel/framework": "4.1.*"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests/php/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
